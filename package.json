{
  "name": "eslint-import-resolver-oxc",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@9.5.0",
  "description": "A simply wrapped `oxc-resolver` for `eslint-plugin-import-x` and `eslint-plugin-import`",
  "author": "Vida Xie<https://github.com/9romise>",
  "license": "MIT",
  "homepage": "https://github.com/9romise/eslint-import-resolver-oxc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9romise/eslint-import-resolver-oxc.git"
  },
  "bugs": {
    "url": "https://github.com/9romise/eslint-import-resolver-oxc/issues"
  },
  "keywords": [
    "eslint",
    "eslint-plugin-import",
    "eslint-plugin-import-x",
    "eslint-import-resolver"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.6.0"
  },
  "scripts": {
    "build": "tsup",
    "prepare": "npx simple-git-hooks",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "dependencies": {
    "oxc-resolver": "^1.10.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vida0905/eslint-config": "^0.3.0",
    "bumpp": "^9.4.1",
    "changelogithub": "^0.13.9",
    "eslint": "^9.7.0",
    "eslint-import-resolver-oxc": "file:",
    "lint-staged": "^15.2.7",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
