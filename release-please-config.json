      "hidden": false
    },
    {
      "type": "docs",
      "section": "Documentation",
      "hidden": true
    },
    {
      "type": "build",
      "section": "Build Related",
      "hidden": false
    },
    {
      "type": "chore",
      "section": "Chores",
      "hidden": true
    },
    {
      "type": "perf",
      "section": "Performance",
      "hidden": false
    },
    {
      "type": "ci",
      "section": "Chores",
      "hidden": true
    },
    {
      "type": "refactor",
      "section": "Chores",
      "hidden": false
    },
    {
      "type": "test",
      "section": "Chores",
      "hidden": false
    }
  ],
  "packages": {
    "*": {
    }
  }
}
